{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "CarRent.Backend"
    }
  },
  "ConnectionStrings": {
    // "LaborDatDb": "Data Source=(localdb)\\MSSQLLocalDB;Database=LaborDat;MultipleActiveResultSets=True;Trusted_Connection=True;"
  }
}
